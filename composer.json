{
    "name": "AlexSchmidhuber/laravel-trait-collection",
    "description": "A trait collection for Laravel",
    "license": "BSD 3 Clause",
    "authors": [
        {
            "name": "Alexander Schmidhuber",
            "email": "alexander.schmidhuber@gmail.com",
            "homepage": "https://github.com/alexschmidhuber"
        }
    ],
    "homepage": "https://github.com/AlexSchmidhuber/laravel-trait-collection",
    "keywords": [
        "Laravel",
        "LaravelTraitCollection"
    ],
    "require": {
        "illuminate/support": "~5|~6",
        "webpatser/laravel-uuid": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "AlexSchmidhuber\\LaravelTraitCollection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexSchmidhuber\\LaravelTraitCollection\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexSchmidhuber\\LaravelTraitCollection\\LaravelTraitCollectionServiceProvider"
            ]
        }
    }
}
